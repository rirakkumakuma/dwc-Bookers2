<%= render 'books/list',books: @books %>

<div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <% @users.each do |user| %>
        <div>
        <tr><td><%=attachment_image_tag user, :profile_image, :fill,60,60,fallback: "no_image.jpg"%>
        <td><%= user.name %></td>
        <td><%= link_to "show",user_path(user.id)  %></td></tr>
        </div>
      </tbody>
        <% end %>

  </tbody>
</table>
    </div>